{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions" : [
        {
            "name" : "client",
            "image": "soumya9v/multi-client",
            "hostname" : "client",
            "essential" : false
        },
        {
            "name" : "server",
            "image": "soumya9v/multi-server",
            "hostname" : "api",
            "essential" : false
        },
        {
            "name" : "worker",
            "image": "soumya9v/multi-worker",
            "essential" : false
        },
        {
            "name" : "nginx",
            "image": "soumya9v/multi-nginx",
            "essential" : true,
            "portMappings": [
                {
                    "hostPort" : 80,
                    "containerPort" : 80
                }
            ],
            "links" : [
                "client", "server"
            ]
        }
    ]
}